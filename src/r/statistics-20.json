      "target": "app/statistics-20/page.tsx"
    },
    {
      "path": "src/components/shadcn-space/blocks/statistics-20/statistics20.tsx",
      "content": "'use client'\r\n\r\nimport dynamic from 'next/dynamic'\r\nimport { Card } from '@/components/ui/card'\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuItem,\r\n  DropdownMenuTrigger,\r\n} from '@/components/ui/dropdown-menu'\r\nimport { HiOutlineDotsVertical } from 'react-icons/hi'\r\n\r\nconst Chart = dynamic(() => import('react-apexcharts'), { ssr: false })\r\n\r\nconst Statistics20 = () => {\r\n  const dropdownItems = ['Action', 'Another action', 'Something else']\r\n  const ChartData: any = {\r\n    series: [\r\n      {\r\n        name: 'Users',\r\n        data: [25, 66, 20, 40, 12, 58, 20],\r\n      },\r\n    ],\r\n    chart: {\r\n      type: 'area',\r\n      height: 80,\r\n      sparkline: {\r\n        enabled: true,\r\n      },\r\n      group: 'sparklines',\r\n      fontFamily: 'inherit',\r\n      foreColor: '#adb0bb',\r\n    },\r\n    colors: ['#38BDF8'],\r\n    stroke: {\r\n      curve: 'smooth',\r\n      width: 2,\r\n    },\r\n    fill: {\r\n      type: 'gradient',\r\n      color: ['#38BDF8'],\r\n      gradient: {\r\n        shadeIntensity: 0,\r\n        inverseColors: false,\r\n        opacityFrom: 0.4,\r\n        opacityTo: 0.8,\r\n        stops: [100],\r\n      },\r\n    },\r\n    markers: {\r\n      size: 0,\r\n    },\r\n    tooltip: {\r\n      theme: 'dark',\r\n      x: {\r\n        show: false,\r\n      },\r\n    },\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <div className='min-h-screen flex flex-col items-center justify-center'>\r\n        <Card className='w-full max-w-sm p-6'>\r\n          <div className='flex items-center justify-between'>\r\n            <h5 className='text-lg font-semibold text-foreground'>\r\n              Annual Profit\r\n            </h5>\r\n            <div>\r\n              <DropdownMenu>\r\n                <DropdownMenuTrigger className='outline-none'>\r\n                  <span className='h-9 w-9 flex justify-center items-center rounded-full hover:bg-accent  hover:text-accent-foreground cursor-pointer'>\r\n                    <HiOutlineDotsVertical size={22} />\r\n                  </span>\r\n                </DropdownMenuTrigger>\r\n                <DropdownMenuContent>\r\n                  {dropdownItems.map((items, index) => {\r\n                    return (\r\n                      <DropdownMenuItem\r\n                        key={index}\r\n                        className='font-normal cursor-pointer'>\r\n                        {items}\r\n                      </DropdownMenuItem>\r\n                    )\r\n                  })}\r\n                </DropdownMenuContent>\r\n              </DropdownMenu>\r\n            </div>\r\n          </div>\r\n\r\n          <div className='bg-sky-400/10 dark:bg-sky-400/10 rounded-md'>\r\n            <div className='p-6 flex justify-between items-center'>\r\n              <p className='text-sm font-normal text-muted-foreground'>\r\n                Conversion Rate\r\n              </p>\r\n              <h4 className='text-xl font-semibold text-foreground'>18.4%</h4>\r\n            </div>\r\n            <div className='h-[80px]'>\r\n              <Chart\r\n                options={ChartData}\r\n                series={ChartData.series}\r\n                type='area'\r\n                height='80px'\r\n                width='100%'\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          <div className='flex flex-col gap-3'>\r\n            <div className='flex justify-between'>\r\n              <div>\r\n                <h6 className='text-base font-normal text-foreground/80 mb-1'>\r\n                  Added to Cart\r\n                </h6>\r\n                <p className='text-sm font-normal text-muted-foreground'>5 clicks</p>\r\n              </div>\r\n              <div>\r\n                <h6 className='text-base font-normal tracking-wide mb-1'>\r\n                  $21,120.70\r\n                </h6>\r\n                <p className='font-medium text-sm text-right text-teal-400'>\r\n                  +13.2%\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div className='flex justify-between'>\r\n              <div>\r\n                <h6 className='text-base font-normal text-foreground/80 mb-1'>\r\n                  Reached to Checkout\r\n                </h6>\r\n                <p className='text-sm font-normal text-muted-foreground'>12 clicks</p>\r\n              </div>\r\n              <div>\r\n                <h6 className='text-base font-normal tracking-wide mb-1'>\r\n                  $16,100.00\r\n                </h6>\r\n                <p className='text-sm font-medium text-right text-red-500'>\r\n                  -7.4%\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </Card>\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Statistics20\r\n",
      "type": "registry:component",
      "target": "components/shadcn-space/blocks/statistics-20/statistics20.tsx"
    }
  ]
}